<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:baselineAligned="false"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/white"
    tools:context="com.example.donglecheckin.CheckInLogs" >

        <RelativeLayout
            android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:layout_weight="1"
        	android:background="@color/white"
        	android:layout_marginTop="0dp">
	            <RelativeLayout 
				    android:id="@+id/imageLayout1"
				    android:layout_width="@dimen/image_layout_width"
				    android:layout_height="fill_parent"
				    android:background="@color/black">

				    <ImageView
				        android:id="@+id/imageView1"
				        android:layout_width="fill_parent"
				        android:layout_height="fill_parent"
				        android:src="@drawable/green"
				        android:scaleType="fitXY"
				        android:adjustViewBounds="true" />

				</RelativeLayout>
			    <LinearLayout
			        android:orientation="vertical"
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:layout_toEndOf="@id/imageLayout1" 
			        android:background="@color/white">

				        <TextView
				            android:id="@+id/status1"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_marginStart="@dimen/status_layout_margin"
				            android:layout_marginTop="@dimen/status_layout_margin"
				            android:layout_weight="@integer/status_weight" />

				        <TextView
				            android:id="@+id/location1"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_marginLeft="@dimen/location_layout_margin"
				            android:layout_marginTop="@dimen/location_layout_margin"
				            android:layout_marginBottom="@dimen/location_layout_margin"
				            android:layout_marginRight="@dimen/location_layout_margin"
				            android:layout_weight="@integer/location_weight"/>

				        <TextView
				            android:id="@+id/timestamp1"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_marginStart="@dimen/timestamp_layout_margin"
				            android:layout_marginTop="@dimen/timestamp_layout_margin"
				            android:layout_weight="@integer/timestamp_weight"/>

			    </LinearLayout>
        </RelativeLayout> 
        <RelativeLayout
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:layout_weight="1">
	            <RelativeLayout 
				    android:id="@+id/imageLayout2"
				    android:layout_width="@dimen/image_layout_width"
				    android:layout_height="match_parent">
				    <ImageView
				            android:id="@+id/imageView2"
				            android:layout_width="fill_parent"
				            android:layout_height="fill_parent"
				            android:src="@drawable/green"
				            android:scaleType="fitXY"
				        	android:adjustViewBounds="true"  />
				</RelativeLayout>
			    <LinearLayout
			        android:orientation="vertical"
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:layout_toEndOf="@id/imageLayout2" >
				        <TextView
				            android:id="@+id/status2"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/status_weight"
				            android:layout_marginStart="@dimen/status_layout_margin"
				            android:layout_marginTop="@dimen/status_layout_margin" />
				        <TextView
				            android:id="@+id/location2"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/location_weight"
				            android:layout_marginStart="@dimen/location_layout_margin"
				            android:layout_marginTop="@dimen/location_layout_margin" />
				        <TextView
				            android:id="@+id/timestamp2"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/timestamp_weight"
				            android:layout_marginStart="@dimen/timestamp_layout_margin"
				            android:layout_marginTop="@dimen/timestamp_layout_margin" />
			    </LinearLayout>
        </RelativeLayout> 
        <RelativeLayout
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:layout_weight="1">
	            <RelativeLayout 
				    android:id="@+id/imageLayout3"
				    android:layout_width="@dimen/image_layout_width"
				    android:layout_height="match_parent">
				    <ImageView
				            android:id="@+id/imageView3"
				            android:layout_width="fill_parent"
				            android:layout_height="fill_parent"
				            android:src="@drawable/green"
				            android:scaleType="fitXY"
				        	android:adjustViewBounds="true" />
				</RelativeLayout>
			    <LinearLayout
			        android:orientation="vertical"
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:layout_toEndOf="@id/imageLayout3" >
				        <TextView
				            android:id="@+id/status3"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/status_weight"
				            android:layout_marginStart="@dimen/status_layout_margin"
				            android:layout_marginTop="@dimen/status_layout_margin" />
				        <TextView
				            android:id="@+id/location3"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/location_weight"
				            android:layout_marginStart="@dimen/location_layout_margin"
				            android:layout_marginTop="@dimen/location_layout_margin" />
				        <TextView
				            android:id="@+id/timestamp3"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/timestamp_weight"
				            android:layout_marginStart="@dimen/timestamp_layout_margin"
				            android:layout_marginTop="@dimen/timestamp_layout_margin" />
			    </LinearLayout>
        </RelativeLayout> 
        <RelativeLayout
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:layout_weight="1">
	            <RelativeLayout 
				    android:id="@+id/imageLayout4"
				    android:layout_width="@dimen/image_layout_width"
				    android:layout_height="match_parent">
				    <ImageView
				            android:id="@+id/imageView4"
				            android:layout_width="fill_parent"
				            android:layout_height="fill_parent"
				            android:src="@drawable/green"
				            android:scaleType="fitXY"
				        	android:adjustViewBounds="true" />
				</RelativeLayout>
			    <LinearLayout
			        android:orientation="vertical"
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:layout_toEndOf="@id/imageLayout4" >
				        <TextView
				            android:id="@+id/status4"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/status_weight"
				            android:layout_marginStart="@dimen/status_layout_margin"
				            android:layout_marginTop="@dimen/status_layout_margin"/>
				        <TextView
				            android:id="@+id/location4"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/location_weight"
				            android:layout_marginStart="@dimen/location_layout_margin"
				            android:layout_marginTop="@dimen/location_layout_margin"/>
				        <TextView
				            android:id="@+id/timestamp4"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/timestamp_weight"
				            android:layout_marginStart="@dimen/timestamp_layout_margin"
				            android:layout_marginTop="@dimen/timestamp_layout_margin"/>
			    </LinearLayout>
        </RelativeLayout>                        
        <RelativeLayout
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:layout_weight="1">
	            <RelativeLayout 
				    android:id="@+id/imageLayout5"
				    android:layout_width="@dimen/image_layout_width"
				    android:layout_height="match_parent">
				    <ImageView
				            android:id="@+id/imageView5"
				            android:layout_width="fill_parent"
				            android:layout_height="fill_parent"
				            android:src="@drawable/green"
				            android:scaleType="fitXY"
				        	android:adjustViewBounds="true" />
				</RelativeLayout>
			    <LinearLayout
			        android:orientation="vertical"
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:layout_toEndOf="@id/imageLayout5" >
				        <TextView
				            android:id="@+id/status5"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/status_weight"
				            android:layout_marginStart="@dimen/status_layout_margin"
				            android:layout_marginTop="@dimen/status_layout_margin"/>
				        <TextView
				            android:id="@+id/location5"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/location_weight"
				            android:layout_marginStart="@dimen/location_layout_margin"
				            android:layout_marginTop="@dimen/location_layout_margin"/>
				        <TextView
				            android:id="@+id/timestamp5"
				            android:layout_width="match_parent"
				            android:layout_height="0dp"
				            android:layout_weight="@integer/timestamp_weight"
				            android:layout_marginStart="@dimen/timestamp_layout_margin"
				            android:layout_marginTop="@dimen/timestamp_layout_margin"/>
			    </LinearLayout>
        </RelativeLayout>                        
</LinearLayout>