					            android:layout_marginTop="@dimen/location_layout_margin"
					            
					            android:gravity="bottom"
					            
					            android:textAppearance="?android:attr/textAppearanceListItemSmall"
	            				android:textSize="10dp"
	            				android:fontFamily="sans-serif-thin" />
					        <TextView
					            android:id="@+id/timestamp2"
					            android:layout_width="match_parent"
					            android:layout_height="0dp"
					            android:layout_weight="@integer/timestamp_weight"
					            android:layout_marginStart="@dimen/timestamp_layout_margin"
					            android:layout_marginTop="@dimen/timestamp_layout_margin"
					            
					            android:gravity="top"
					            
					            android:textAppearance="?android:attr/textAppearanceListItemSmall"
	            				android:textSize="10dp"
	            				android:fontFamily="sans-serif-thin" />
					    </LinearLayout>
			    </LinearLayout>
        </RelativeLayout> 
