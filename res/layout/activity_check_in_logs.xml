<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:baselineAligned="false"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@color/white"
    tools:context="com.example.donglecheckin.CheckInLogs" >

        <RelativeLayout
            android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:layout_weight="1"
        	android:background="@color/white"
        	android:layout_marginTop="0dp"
        	
        	android:paddingLeft="@dimen/full_layout_left_padding">
			    <LinearLayout
			        android:id="@+id/ll1"
			        android:layout_width="match_parent"
			        android:layout_height="match_parent"
			        android:background="@drawable/linear_back"
			        android:paddingLeft="@dimen/layout_left_padding"
			        android:paddingTop="@dimen/layout_top_padding"
			        android:paddingBottom="@dimen/layout_bottom_padding"
			        android:paddingRight="@dimen/layout_right_padding">

				        <TextView
				            android:id="@+id/status1"
				            android:layout_width="0dp"
				            android:layout_weight="1.1"
				            android:layout_height="fill_parent"
				            android:textAlignment="gravity"
				            android:gravity="center"
				            android:text="Test Status"
				            android:layout_marginStart="@dimen/status_layout_margin"
				            android:layout_marginTop="@dimen/status_layout_margin"
				            
				            
				            android:textAppearance="?android:attr/textAppearanceLarge"
				            android:fontFamily="sans-serif-thin" />
				       <ImageView
					        android:id="@+id/imageView1"
					        android:layout_width="0dp"
				            android:layout_weight="1"
					        android:layout_height="fill_parent"
					        android:src="@drawable/green"
					        android:scaleType="fitCenter"
					        android:adjustViewBounds="true" />

				        <LinearLayout 
				            android:layout_width="0dp"
				            android:layout_weight="1"
				            android:layout_height="fill_parent"
				            android:orientation="vertical">
					        <TextView
					            android:id="@+id/location1"
					            android:layout_width="match_parent"
					            android:layout_height="0dp"
					            android:text="Test Location"
					            android:gravity="bottom"
					            android:layout_marginLeft="@dimen/location_layout_margin"
					            android:layout_marginTop="@dimen/location_layout_margin"
					            android:layout_marginBottom="@dimen/location_layout_margin"
					            android:layout_marginRight="@dimen/location_layout_margin"
					            android:layout_weight="@integer/location_weight"
					            
					            android:textAppearance="?android:attr/textAppearanceListItemSmall"
	            				android:textSize="10dp"
	            				android:fontFamily="sans-serif-thin"/>
	
					        <TextView
					            android:id="@+id/timestamp1"
					            android:layout_width="match_parent"
					            android:layout_height="0dp"
					            android:text="Test Timestamp"
					            android:gravity="top"
					            android:layout_marginStart="@dimen/timestamp_layout_margin"
					            android:layout_marginTop="@dimen/timestamp_layout_margin"
					            android:layout_weight="@integer/timestamp_weight"
					            
					            android:textAppearance="?android:attr/textAppearanceListItemSmall"
	            				android:textSize="10dp"
	            				android:fontFamily="sans-serif-thin"/>
						</LinearLayout>
			    </LinearLayout>
        </RelativeLayout> 
        <RelativeLayout
            android:layout_width="fill_parent"
        	android:layout_height="wrap_content"
        	android:layout_weight="1"
        	android:paddingLeft="@dimen/full_layout_left_padding">

